#include<stdio.h>
#include<string.h>
voidswap(char*x,char*y)
{
chartemp;
temp=*x;
*x=*y;
*y=temp;
}
voidpermute(char*a,inti,intn)
{
intj;
if(i==n)
printf("%s\n",a);
else{
for(j=i;j<=n;j++)
{
swap((a+i),(a+j));
permute(a,i+1,n);
swap((a+i),(a+j));
}
}
}
intmain()
{
chara[20];
intn;
printf("Enterastring:");
scanf("%s",a);
n=strlen(a);
printf("Permutaions:\n");
permute(a,0,n-1);
getchar();
return0;
}
